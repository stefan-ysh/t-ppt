{
  "buildCommand": "node build.js",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/ppt/([^/]+)/(.*)", "destination": "/ppt/$1/$2" },
    { "source": "/ppt/([^/]+)", "destination": "/ppt/$1/index.html" }
  ]
}